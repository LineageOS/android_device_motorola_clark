		<ctl name="AIF1TX1 Input 1" value="SLIMRX1" />
		<ctl name="AIF1TX2 Input 1" value="SLIMRX2" />
		<!-- Select Left channel on Left IC bypass mode -->
		<ctl name="NXP Left Ch Select" value="1" />
		<!-- Select Right channel on Right IC bypass mode -->
		<ctl name="NXP Right Ch Select" value="2" />
		<!-- Disable earpiece mode -->
		<ctl name="TFA9890 earpiece mode" value="Off" />
	</path>

	<path name="speaker-reverse">
		<ctl name="SLIM_0_RX Channels" value="Two" />
		<ctl name="AIF1TX1 Input 1" value="SLIMRX1" />
		<ctl name="AIF1TX2 Input 1" value="SLIMRX2" />
		<!-- Select Right channel on Left IC bypass mode -->
		<ctl name="NXP Left Ch Select" value="2" />
		<!-- Select Left channel on Right IC bypass mode -->
		<ctl name="NXP Right Ch Select" value="1" />
		<!-- Disable earpiece mode -->
		<ctl name="TFA9890 earpiece mode" value="Off" />
	</path>

	<path name="speaker-safe">
		<!-- Bottom speaker only -->
		<!-- Enable Mixers to power up Right IC only -->
		<ctl name="SLIM_0_RX Channels" value="One" />
		<ctl name="AIF1TX2 Input 1" value="SLIMRX1" />
		<!-- Select mono channel on Right IC -->
		<ctl name="NXP Right Ch Select" value="3" />
		<!-- Disable earpiece mode this toggles gpio -->
		<ctl name="TFA9890 earpiece mode" value="Off" />
		<ctl name="Left SPK Mux" value="Off" />
	</path>

